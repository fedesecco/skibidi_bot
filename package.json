{
  "name": "gangyoo",
  "version": "0.1.0",
  "description": "Telegram group bot for events and RSVPs",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --loader ts-node/esm --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "sethook": "node scripts/set-webhook.js",
    "unsethook": "node scripts/unset-webhook.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "chat",
    "group",
    "events"
  ],
  "author": "Federico Secco",
  "license": "MIT",
  "homepage": "https://github.com/fedesecco/gangyoo#readme",
  "bugs": {
    "url": "https://github.com/fedesecco/gangyoo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fedesecco/gangyoo.git"
  },
  "dependencies": {
    "@grammyjs/conversations": "^2.1.1",
    "@grammyjs/i18n": "^1.1.2",
    "@supabase/supabase-js": "^2.39.7",
    "dotenv": "^16.4.0",
    "grammy": "^1.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
